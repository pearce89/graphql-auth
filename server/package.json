{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "11.x"
  },
  "scripts": {
    "dev": "nodemon -L --exec babel-node --inspect ./src/index.js --ext js,gql,env | pino-pretty -t -c -f -l",
    "test": "NODE_ENV=test jest --watchAll --runInBand --silent",
    "test-debugger": "NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand",
    "jest": "NODE_ENV=test jest --ci --runInBand --silent --reporters=default --reporters=jest-junit",
    "build": "babel src --out-dir dist --copy-files",
    "start": "node dist/index.js",
    "knex": "babel-node node_modules/.bin/knex --knexfile ./src/db/config.js",
    "knex-migrate": "babel-node node_modules/.bin/knex-migrate --knexfile ./src/db/config.js",
    "heroku-knex": "knex --knexfile ./dist/db/config.js",
    "heroku-migrate": "knex-migrate --knexfile ./dist/db/config.js",
    "heroku-deploy": "cd ../ && git subtree push --prefix server heroku-server master || true",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "db:create": "run-func src/db/tasks.js createDb",
    "db:clear": "run-func src/db/tasks.js truncateDb",
    "db:drop": "run-func src/db/tasks.js dropDb",
    "db:migrate": "babel-node node_modules/.bin/knex-migrate --knexfile ./src/db/config.js",
    "db:seed": "babel-node node_modules/.bin/knex --knexfile ./src/db/config.js seed:run"
  },
  "jest": {
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "verbose": true
  },
  "keywords": [],
  "author": "Sergei Anisimov",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.4.1",
    "apollo-server": "^2.6.7",
    "bcrypt": "^3.0.6",
    "cross-fetch": "^3.0.2",
    "dotenv": "^7.0.0",
    "faker": "^4.1.0",
    "graphql": "^14.4.2",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^5.7.3",
    "graphql-tools": "^4.0.5",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "knex-db-manager": "^0.5.0",
    "knex-migrate": "^1.7.2",
    "lodash": "^4.17.21",
    "objection": "^1.6.8",
    "objection-before-and-unique": "^0.5.0",
    "objection-guid": "^3.0.2",
    "password-validator": "^4.1.1",
    "pg": "^7.11.0",
    "pg-escape": "^0.2.0",
    "pino": "^5.12.3",
    "pino-pretty": "^2.6.1",
    "run-func": "^1.0.2",
    "schemaglue": "^4.0.4",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-plugin-root-import": "^6.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "factory-girl": "^5.0.4",
    "factory-girl-objection-adapter": "^1.0.0",
    "get-port": "^4.2.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  }
}
